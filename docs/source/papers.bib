@article{charonRosinNaturalTerpenes2022,
  title = {Rosin Natural Terpenes as Processing Aid for Polyhydroxyalkanoate: {{Thermal}}, Mechanical, and Viscoelastic Properties},
  shorttitle = {Rosin Natural Terpenes as Processing Aid for Polyhydroxyalkanoate},
  author = {Charon, Ga{\"e}tan and Peixinho, Jorge and Michely, Laurent and Guinault, Alain and Langlois, Val{\'e}rie},
