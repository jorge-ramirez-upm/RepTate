@article{qiaoUnderstandingRheologyPolymer2023,
  title = {Understanding the {{Rheology}} of {{Polymer}}{\textendash}{{Polymer Interfaces Covered}} with {{Janus Nanoparticles}}: {{Polymer Blends}} versus {{Particle Sandwiched Multilayers}}},
  shorttitle = {Understanding the {{Rheology}} of {{Polymer}}{\textendash}{{Polymer Interfaces Covered}} with {{Janus Nanoparticles}}},
  author = {Qiao, Huawei and Zheng, Botuo and Zhong, Gang and Li, Zhicong and Cardinaels, Ruth and Moldenaers, Paula and Lamnawar, Khalid and Maazouz, Abderrahim and Liu, Canpei and Zhang, Huagui},
  year = {2023},
